<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dzh.dongbao.portal.web.mapper.UmsMemberMapper">
    <select id="selectByName" resultType="com.dzh.dongbao.portal.web.entity.UmsMember" parameterType="String">
        select *
        from ums_member
        where username = #{username}
    </select>
</mapper>
